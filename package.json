{
  "name": "enstruct",
  "version": "1.0.0",
  "description": "Create binary data based on data-type specific encoders.",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "files": [
    "build/**/*.d.ts",
    "build/**/*.js",
    "*.d.ts",
    "LICENSE",
    "package.json",
    "README.md",
    "specification.md"
  ],
  "scripts": {
    "clean": "rimraf build",
    "build": "tsc -p .",
    "watch": "tsc -w -p .",
    "prepare": "npm run clean && npm run build",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "binary",
    "struct",
    "structure",
    "parser"
  ],
  "author": "Saeed M Rad",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "dependency-graph": "^0.9.0"
  }
}
